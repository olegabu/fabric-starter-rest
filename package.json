{
  "name": "fabric-starter-rest",
  "version": "1.0.0",
  "description": "rest api server and client for hyperledger fabric built with nodejs sdk",
  "main": "fabric-starter-client.js",
  "scripts": {
    "start-peer0": "cd features/fixtures/network/org1.example.test; ./start-peer0.sh",
    "start-peer2-agent": "DOTENV_CONFIG_PATH=features/fixtures/network/org1.example.test/peer2/.peer2-test-env FABRIC_STARTER_PWD=$PWD/features/fixtures/network/org1.example.com/peer2 node -r dotenv/config app.js",
    "preintegration": "npm run start-peer2-agent &",
    "integration": "npm run cucumber features/peer_component_deploy.feature || true",
    "postintegration": "pkill -f node",
    "cucumber": "cucumber-js",
    "test": "DISCOVER_AS_LOCALHOST=true jest",
    "test-mocha": "DISCOVER_AS_LOCALHOST=true mocha",
    "dev": "./start-dev.sh",
    "start": "node app.js",
    "dev-test": "npm run start-peer0 && cucumber-js features/tests/ledger-storage-test.feature"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/gost-deps/",
      "<rootDir>/node_modules/"
    ]
  },
  "repository": "https://github.com/olegabu/fabric-starter-rest",
  "author": "Oleg Abdrashitov <olegabu73@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "base64-stream": "^1.0.0",
    "body-parser": "^1.19.0",
    "channel.js": "0.0.2",
    "chmod-plus": "0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "enumify": "^1.0.4",
    "envsub": "^3.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fabric-ca-client": "^1.4.4",
    "fabric-client": "file:./gost-deps/fabric-client",
    "fabric-cryptosuite-gost": "file:./gost-deps/fabric-cryptosuite-gost",
    "ffi": "^2.3.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "json5": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "log4js": "^6.3.0",
    "minipass": "^3.1.3",
    "multer": "^1.4.2",
    "npm": "^6.11.3",
    "ref": "^1.3.5",
    "ref-array": "^1.2.0",
    "rewire": "^4.0.1",
    "serve-favicon": "^2.5.0",
    "serve-index": "^1.9.1",
    "shelljs": "^0.8.4",
    "sinon": "^7.5.0",
    "socket.io": "^2.3.0",
    "stream3-concat": "^0.2.0",
    "tape": "^4.11.0",
    "tape-promise": "^4.0.0",
    "tar": "^6.1.0",
    "tar-transform": "^1.0.0",
    "tmp-promise": "^3.0.2",
    "unzipper": "^0.10.5",
    "url-parse-lax": "^3.0.0",
    "x509": "^0.3.4"
  },
  "devDependencies": {
    "cucumber": "^6.0.5",
    "docker-compose": "^0.23.10",
    "express-swagger-generator": "^1.1.15",
    "jest": "^25.5.4",
    "mocha": "^7.2.0",
    "nock": "^11.9.1",
    "nodemon": "^1.19.1",
    "randomstring": "^1.1.5",
    "supertest": "^6.1.3"
  },
  "nodemonConfig": {
    "ignore": [
      "./upload",
      "./crypto-config",
      "msp"
    ]
  }
}
